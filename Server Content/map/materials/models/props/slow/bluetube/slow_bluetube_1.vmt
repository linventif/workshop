"UnlitGeneric"
{
	"$basetexture" "models/props/slow/bluetube/slow_bluetube_1"
	"$translucent" 1
	"$additive" 1
	"$model" 1
	"$nocull" 1

	"$envmap" "models/props/slow/bluetube/slow_bluetube_spec"

	"$envmaptint" "[.6 .6 .6]" 
	"$envmapcontrast" 1
	"$envmapsaturation" 1.1


   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
	 "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 15.00
     	}
	}
}
