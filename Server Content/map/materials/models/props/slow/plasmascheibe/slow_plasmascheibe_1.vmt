"UnlitGeneric"
{
	"$basetexture" "models/props/slow/plasmascheibe/slow_plasmascheibe_1"
	"$translucent" 1
	"$nocull" 1
	"$model" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[1 1 1]" 
	"$envmapcontrast" .5
	"$envmapsaturation" 0.85


   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
	 "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 8.00
     	}
	}
}