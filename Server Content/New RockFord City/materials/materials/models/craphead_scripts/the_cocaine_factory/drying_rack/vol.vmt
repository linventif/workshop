"unlittwotexture"
{
	"$baseTexture" "models\craphead_scripts\the_cocaine_factory\drying_rack/vol_lightmask"
	"$texture2" "models\craphead_scripts\the_cocaine_factory\drying_rack/dust"
    "$texture2transform" "center .5 .5 scale 1 1 rotate 0 translate 0 0"
	"$nocull" 1	
	"$additive" 1
	"$no_fullbright" 1	

		"$scale" "[.8 .8]"
        "$translate" "[0 0]"

        "Proxies"
        {
            "LinearRamp"
            {
                "rate" -0.01
                "initialValue" 0
                "resultVar" "$translate[1]"
            }
			
            "TextureTransform"
            {
                "translateVar" "$translate"
				"scaleVar" "$scale"
                "resultVar" "$texture2transform"
            }			
        }
}

