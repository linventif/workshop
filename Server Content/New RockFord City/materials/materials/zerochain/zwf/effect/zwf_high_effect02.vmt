"UnlitGeneric"
{
	"$baseTexture"			"zerochain/zwf/effect/zwf_high_effect02_diff"

	"$ignorez" "1"
	//"$additive" "1"

    "$translate" "[0.0 0.0]"
    "$center" "[0.5 0.5]"
	
	"$def_scale" "[1 1]"
	
	"$angle" "0"
	
	"Proxies"
	{
		
		"Sine"
		{
            "sineperiod" 0.7
            "sinemin" 0.1
			"sinemax" 0.2
			"timeoffset" 0.1
            "resultVar" "$alpha"
		}	
		
		"LinearRamp"
		{
            "rate" 0.1
            "initialValue" 0.0
            "resultVar" "$translate[0]"
		}
	
		"LinearRamp"
		{
            "rate" 0.1
            "initialValue" 0.0
            "resultVar" "$translate[1]"
		}
	

		"TextureTransform"
		{
            "translateVar" "$translate"
			"scaleVar"  "$def_scale"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$basetexturetransform"
		}		
	}
	
}
