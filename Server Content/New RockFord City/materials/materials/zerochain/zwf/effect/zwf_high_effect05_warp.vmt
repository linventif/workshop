"Refract"
{
	"$refractamount" "0.001"
	
	"$bluramount" "0.5"
	"$ignorez" "1"

	"$scale" "[1 1]"

	"$normalmap"			"zerochain/zwf/effect/zwf_high_effect05_nrm"
	
	
	"$translate" "[0.0 0.0]"
    "$center" "[0.5 0.5]"
	
	"$def_scale" "[1 1]"
	
	"$angle" "0"
	
	"Proxies"
	{
		
		
		"Sine"
		{
            "sineperiod" 6
            "sinemin" -180
			"sinemax" 180
			"timeoffset" 0.1
            "resultVar" "$angle"
		}
		
	
		
        "TextureTransform"
		{
            "translateVar" "$translate"
			"scaleVar"  "$def_scale"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$bumptransform"
		}		
	}
}
