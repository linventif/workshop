"Refract"
{
	"$refractamount" "0.001"
	
	"$bluramount" "0.5"
	"$ignorez" "1"

	"$scale" "[1 1]"
	
	"$normalmap"			"zerochain/zwf/effect/zwf_high_effect01_nrm"
	
	
	"$translate" "[0.0 0.0]"
    "$center" "[0.5 0.5]"
	
	"$def_scale" "[1 1]"
	
	"$angle" "0"
	
	"Proxies"
	{
		
		"LinearRamp"
		{
            "rate" 15
            "initialValue" 0.0
            "resultVar" "$angle"
		}

		
        "TextureTransform"
		{
            "translateVar" "$translate"
			"scaleVar"  "$def_scale"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$bumptransform"
		}		
	}
}
