"VertexLitGeneric"
{

	"$baseTexture"			"zerochain/props_yeastbeast/yb_barrel/yb_barrel_liquid"

	"$surfaceprop" "slime"
	"$model" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[0.1 0.1 0.1]"
	"$envmapcontrast" "0"
	
	$phong 1

        $phongexponent 1
        $phongboost 0.4
		//$phongtint "[0.1 0.7 1]"
        $phongfresnelranges "[1 2 3]"

	// Use scrolling animated textures on DX9 hardware
	">=DX90"
	{
		"$bumpmap" "Nature/slime_normal"
		"$normalmapalphaenvmapmask" "1"

		"Proxies"
		{
			"AnimatedTexture"
			{
				"animatedtexturevar" "$bumpmap"
				"animatedtextureframenumvar" "$bumpframe"
				"animatedtextureframerate" 21.00
			}

		}
	}
}
