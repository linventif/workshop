"VertexLitGeneric"
{
	"$baseTexture"			"zerochain/props_trashman/ztm_conveyorbelt_diff"


	"$bumpMap"				"zerochain/props_trashman/ztm_conveyorbelt_nrm"
	"$normalmapalphaenvmapmask" "1"
	
	"$surfaceprop" "metal"
	"$model" 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.1 .1 .1]"
	"$envmapfresnel" "0"
	
	$phong 1

        $phongexponent 15
        $phongboost 1
        $phongtint "[.1 .1 .1]"
        $phongfresnelranges "[1 2 3]"

		$rimlight               1
		$rimlightexponent       0.01
		$rimlightboost         1
		
		
		
		
		 "Proxies"
        {
                "TextureScroll"
                {
                        "texturescrollvar" "$baseTextureTransform"
                        "texturescrollrate" .5
                        "texturescrollangle" 90
                }
				
				"TextureScroll"
                {
                        "texturescrollvar" "$bumpMapTransform"
                        "texturescrollrate" .5
                        "texturescrollangle" 90
                }
        }	
}